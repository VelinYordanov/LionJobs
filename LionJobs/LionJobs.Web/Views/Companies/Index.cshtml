@model IEnumerable<LionJobs.Web.ViewModels.ICompanyViewModel>
<h4>Companies:</h4>
<div class="row">
    @foreach (var company in Model)
    {

        <div class="col s4">
            <div class="thumbnail">
                <img src="@company.UserImageUrl" class="img-circle responsive-img" alt="User Image">
                <div class="caption">
                    <h3>@company.CompanyName</h3>
                    <p>@company.Description</p>
                </div>
            </div>
        </div>
    }
</div>





